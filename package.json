{
  "name": "omedacity-js",
  "description": "A simple JavaScript SDK to access the OmedaCity API.",
  "keywords": [
    "omedacity",
    "predecessor",
    "javascript",
    "typescript"
  ],
  "author": "MrBetaMax",
  "license": "MIT",
  "repository": {
    "type": "git",
    "directory": "src",
    "url": "git+https://github.com/dewald-els/omedacity-js.git"
  },
  "private": false,
  "version": "0.6.2",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": {
        "require": "./dist/index.d.ts",
        "default": "./dist/index.d.ts"
      },
      "default": {
        "require": "./dist/index.js",
        "default": "./dist/index.js"
      }
    },
    "./types": {
      "types": {
        "require": "./dist/types/index.js",
        "default": "./dist/types/index.js"
      },
      "default": {
        "require": "./dist/types/index.js",
        "default": "./dist/types/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest __tests__/",
    "clean:dist": "rm -rf ./dist",
    "clean:all": "rm -rf ./dist && rm -rf node_modules"
  },
  "engines": {},
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "babel-jest": "^29.7.0",
    "i": "^0.3.7",
    "jest": "^29.7.0",
    "npm": "^10.2.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.2"
  }
}
